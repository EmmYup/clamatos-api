    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "sails-generate-eslintrc":
      "https://github.com/EmmanuelYupit/sails-generate-eslintrc.git",
    "sails-memory": "^0.10.7",
    "sails-mongo": "^0.12.2"
